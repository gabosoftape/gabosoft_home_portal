                                        <a class="nav-item nav-link ts-scroll btn btn-sm ml-3 px-3 bd-ss ts-width__auto btn_activo_1_dark" href="/web/login">Ingresar</a>
                                    </div>
                                    <!--end navbar-nav-->
                                </div>
                                <!--end collapse-->
                            </div>
                            <!--end container-->
                        </nav>
                        <!--end navbar-->
                    </header>
                    <!--end #hero-->

                    <!--*********************************************************************************************************-->
                    <!--************ CONTENT ************************************************************************************-->
                    <!--*********************************************************************************************************-->
                    <main id="ts-content" class="ts-full-screen">
                        <t t-raw="0"/>
                    </main>
                    <!--end #content-->

